import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String S = sc.nextLine().trim();
        String S1 = sc.nextLine().trim();
        System.out.print(S+S1);

    }
}
